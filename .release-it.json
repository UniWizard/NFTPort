{
  "git": {
    "commitMessage": "'ðŸš€ release v${version}'"
  },
  "github": {
    "release": true
  },
  "npm": false,
  "plugins": {
    "@release-it/conventional-changelog": {
      "infile": "CHANGELOG.md",
      "header": "# Changelog",
      "ignoreRecommendedBump": true,
      "preset": {
        "name": "conventionalcommits",
        "types": [
          {
            "type": "feat",
            "section": "Features"
          },
          {
            "type": "fix",
            "section": "Bug Fixes"
          },
          {}
        ]
      }
    }
  }
}
